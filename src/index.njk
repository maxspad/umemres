{% for row in grid %}
<div class="row">
    {% for col in row %}
    <div class="col-xl border px-4 py-2 m-2 bg-light">
        <h2>{{ col.title }}</h2>
        <ul>
            {% for link in col.links %}
            <li><a href="{{ link.href }}", target="_blank">{{ link.text }}</a></li>
            {% endfor %}
        </ul>
    </div>
    {% endfor %}
</div>
{% endfor %}
